page{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

.wraper{
  width: 100%;
  height: 100%;
  font-size: 14px;
  color: #FFF;
}

.memberRanking-title{
  padding: 10px 0;
  overflow: hidden;
  border-bottom: 1px solid #F0F0F2;
}

.memberRanking-title > view{
  float: left;
  width: 33.33%;
  text-align: center;
  color: #444;
}

.memberRanking-title :first-child{
  text-align: left;
}

.memberRanking-title :last-child{
  text-align: right;
}

.memberRanking-item{
  padding: 10px 0;
  overflow: hidden;
  border-bottom: 1px solid #F0F0F2;
}

.memberRanking-item > view{
  float: left;
  width: 33.33%;
  color: #444;
}

.ranking{
  height: 30px;
  line-height: 30px;
  font-size: 12px;
}

.ranking > image{
  margin-left: 12px;
  width: 27px;
  height: 30px;
}

.memberInfo{
  overflow: hidden;
}

.memberInfo-image{
  float: left;
  margin-left: 20px;
  width: 30px;
  height: 30px;
  border-radius: 50%;
}

.memberInfo-name{
  float: left;
  margin-left: 10px;
  width: 56px;
  height: 30px;
  line-height: 30px;
  overflow: hidden;
}

.count{
  height: 30px;
  text-align: right;
  line-height: 30px;
}

.count > text{
  color: #BBB;
}

