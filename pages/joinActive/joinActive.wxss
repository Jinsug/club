page{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

.wraper{
  width: 100%;
  height: 100%;
  font-size: 14px;
  color: #FFF;
  background-color: #F0F0F2;
}

.activeInfo{
  padding: 10px;
  overflow: hidden;
  background-color: #FFF;
}

.activeImage{
  float: left;
  width: 135px;
  height: 77px;
}

.activeInfo-content{
  float: left;
  margin-left: 20px;
}

.activeName{
  margin-top: 5px;
  color: #444;
}

.activeTarget{
  margin-top: 10px;
  font-size: 12px;
  color: #BBB;
}

.activeCreator{
  margin-top: 5px;
  font-size: 12px;
  color: #BBB;
}

.activeStartTime{
  margin-top: 10px;
  overflow: hidden;
  background-color: #FFF;
}

.activeStartTime-left{
  float: left;
  color: #444;
}

.activeStartTime-right{
  float: right;
  
}
