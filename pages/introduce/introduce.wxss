page{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

.wraper{
  width: 100%;
  height: 100%;
  font-size: 14px;
  color: #FFF;
}

.workDate-title{
  padding: 10px;
  color: #444;
  border-bottom: 1px solid #F0F0F2;
}

.workDate-body{
  padding: 10px;
  color: #BBB;
}

.workDate-time{
  font-size: 12px;
}

.workDate-week{
  font-size: 12px;
}

.project-title{
  padding: 10px;
  color: #444;
  border-bottom: 1px solid #F0F0F2;
}

.project-item{
  padding: 10px;
  overflow: hidden;
}

.project-image{
  float: left;
  width: 114px;
  height: 68px;
}

.project-content{
  float: right;
  width: 60%;
}

.project-name{
  color: #444;
}

.project-remark{
  margin-top: 5px;
  color: #BBB;
  font-size: 12px;
}
